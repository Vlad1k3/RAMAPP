<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        x:Class="moduleApp.ManualWindow"
        Title="Мануал"
        Width="600"
        Height="400"
        CornerRadius="10"
        Background="#302e28"
        WindowStartupLocation="CenterScreen"
        >


    <Grid>
        <StackPanel Spacing="20" VerticalAlignment="Top" HorizontalAlignment="Center">
            <Button Click="DisplayEventsForUser" Content="Показать историю" />
            <Button Click="DeleteAllEvents" Content="Удалить историю" />
        </StackPanel>

        <StackPanel Spacing="50" HorizontalAlignment="Center" VerticalAlignment="Center">          
            <ListBox x:Name="ResultsListBox"></ListBox>
        </StackPanel>
    </Grid>


    

</Window>
